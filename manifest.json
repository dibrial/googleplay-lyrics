{
  "name": "Lyria for Google Music (TM)",
  "version": "1.0.4",
  "description": "Displays lyrics for the current song of Google Music (TM)",
  "background_page": "background.html",
  "icons": { "128": "icon.png" },
  "content_scripts": [
    {
      "matches": ["*://music.google.com/music/listen*"],
      "css": ["contentscript-theme.css"],
      "js": ["jquery.js","contentscript.js"]

    }
  ],
  "permissions": [
     "http://music.google.com/*", "http://lyrics.wikia.com/*"
  ]
}


